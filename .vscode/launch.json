{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
    {
        "name": ".NET Core Launch (console)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/cli/bin/Debug/net10.0/arnold.dll",
        "args": ["@${workspaceFolder}/cli/debug-params.rsp"],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "console": "internalConsole",
        "envFile": "${workspaceFolder}/.env"
    }, {
        "name": ".NET WebAPI Launch (api)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/api/bin/Debug/net10.0/arnold-api.dll",
        "args": [""],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "console": "internalConsole",
        "envFile": "${workspaceFolder}/.env"
    }
    ]
}